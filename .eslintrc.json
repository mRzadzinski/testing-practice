{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "no-use-before-define": "off",
        "no-continue": "off",
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "no-undef": "off"
    }
}
